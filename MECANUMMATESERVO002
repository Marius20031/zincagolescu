                    BleftDrive.setPower(-power);
                    TrightDrive.setPower(-power);
                    BrightDrive.setPower(-power);
                    if (gamepad1.left_trigger != 0)
                        power = 0.5;
                    if (gamepad1.left_trigger == 0)
                        power = 0.3;
                    if (!gamepad1.dpad_up) {
                        TleftDrive.setPower(0);
                        BleftDrive.setPower(0);
                        TrightDrive.setPower(0);
                        BrightDrive.setPower(0);
                        power = 0.5;
                        break;
                    }
