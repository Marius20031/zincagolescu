                        TleftDrive.setPower(0);
                        BleftDrive.setPower(0);
                        TrightDrive.setPower(0);
                        BrightDrive.setPower(0);
                        break;
                    }
                }
                while (gamepad1.right_bumper) {
                    TleftDrive.setPower(-0.35);
                    BleftDrive.setPower(-0.35);
                    TrightDrive.setPower(0.35);
                    BrightDrive.setPower(0.35);
                    if (!gamepad1.left_bumper) {
                        TleftDrive.setPower(0);
                        BleftDrive.setPower(0);
                        TrightDrive.setPower(0);
                        BrightDrive.setPower(0);
                        break;
                    }
                }
            }
            while (gamepad2.a) {
                Paralelogram.setPower(1);
                sleep(100);
                if(!(gamepad2.a)) {
                    Paralelogram.setPower(0);}
                while (gamepad1.dpad_up) {
                    TleftDrive.setPower(-power);
                    BleftDrive.setPower(-power);
                    TrightDrive.setPower(-power);
                    BrightDrive.setPower(-power);
                    if (gamepad1.left_trigger != 0)
                        power = 0.5;
                    if (gamepad1.left_trigger == 0)
                        power = 0.3;
                    if (!gamepad1.dpad_up) {
                        TleftDrive.setPower(0);
                        BleftDrive.setPower(0);
                        TrightDrive.setPower(0);
                        BrightDrive.setPower(0);
                        power = 0.5;
                        break;
                    }
                }


                while (gamepad1.dpad_down) {
                    TleftDrive.setPower(power);
                    BleftDrive.setPower(power);
                    TrightDrive.setPower(power);
                    BrightDrive.setPower(power);
                    if (gamepad1.left_trigger != 0)
                        power = 0.5;
                    if (gamepad1.left_trigger == 0)
                        power = 0.3;
                    if (!gamepad1.dpad_down) {
                        TleftDrive.setPower(0);
                        BleftDrive.setPower(0);
                        TrightDrive.setPower(0);
                        BrightDrive.setPower(0);
                        power = 0.5;
                        break;
                    }
                }

                while (gamepad1.dpad_left) {
                    TleftDrive.setPower(power);
                    BleftDrive.setPower(-power);
                    TrightDrive.setPower(-power);
                    BrightDrive.setPower(power);
                    if (gamepad1.left_trigger != 0)
                        power = 0.9;
                    if (gamepad1.left_trigger == 0)
                        power = 0.5;
                    if (!gamepad1.dpad_left) {
                        TleftDrive.setPower(0);
                        BleftDrive.setPower(0);
                        TrightDrive.setPower(0);
                        BrightDrive.setPower(0);
                        power = 0.9;
                        break;
                    }
                }

                while (gamepad1.dpad_right) {
                    TleftDrive.setPower(-power);
                    BleftDrive.setPower(power);
                    TrightDrive.setPower(power);
                    BrightDrive.setPower(-power);
                    if (gamepad1.left_trigger != 0)
                        power = 0.9;
                    if (gamepad1.left_trigger == 0)
                        power = 0.5;
                    if (!gamepad1.dpad_right) {
                        TleftDrive.setPower(0);
                        BleftDrive.setPower(0);
                        TrightDrive.setPower(0);
                        BrightDrive.setPower(0);
                        power = 0.9;
                        break;
                    }
                }

                while (gamepad1.y) {
                    TleftDrive.setPower(0.7);
                    BrightDrive.setPower(0.7);
                    if (!(gamepad1.y)) {
                        TleftDrive.setPower(0);
                        BrightDrive.setPower(0);
                        break;
                    }
                }
                while (gamepad1.a) {
                    BleftDrive.setPower(0.7);
                    TrightDrive.setPower(0.7);
                    if (!(gamepad1.a)) {
                        BleftDrive.setPower(0);
                        TrightDrive.setPower(0);
                        break;
                    }
                }
                while (gamepad1.x) {
                    TleftDrive.setPower(-0.7);
                    BrightDrive.setPower(-0.7);
                    if (!(gamepad1.x)) {
                        TleftDrive.setPower(0);
                        BrightDrive.setPower(0);
                        break;
                    }
                }
                while (gamepad1.b) {
                    BleftDrive.setPower(-0.7);
                    TrightDrive.setPower(-0.7);
                    if (!(gamepad1.b)) {
                        BleftDrive.setPower(0);
                        TrightDrive.setPower(0);
                        break;
                    }
                }
                while (gamepad1.left_bumper) {
                    TleftDrive.setPower(0.35);
                    BleftDrive.setPower(0.35);
                    TrightDrive.setPower(-0.35);
                    BrightDrive.setPower(-0.35);
                    if (!gamepad1.right_bumper) {
                        TleftDrive.setPower(0);
                        BleftDrive.setPower(0);
                        TrightDrive.setPower(0);
                        BrightDrive.setPower(0);
                        break;
                    }
                }
                while (gamepad1.right_bumper) {
                    TleftDrive.setPower(-0.35);
                    BleftDrive.setPower(-0.35);
                    TrightDrive.setPower(0.35);
                    BrightDrive.setPower(0.35);
                    if (!gamepad1.left_bumper) {
                        TleftDrive.setPower(0);
                        BleftDrive.setPower(0);
                        TrightDrive.setPower(0);
                        BrightDrive.setPower(0);
                        break;
                    }
                }

            }
            while (gamepad2.y) {
                Paralelogram.setPower(-1);
                sleep(100);
                if(!(gamepad2.y)) {
                    Paralelogram.setPower(0);}
                while (gamepad1.dpad_up) {
                    TleftDrive.setPower(-power);
                    BleftDrive.setPower(-power);
                    TrightDrive.setPower(-power);
                    BrightDrive.setPower(-power);
                    if (gamepad1.left_trigger != 0)
                        power = 0.5;
                    if (gamepad1.left_trigger == 0)
                        power = 0.3;
                    if (!gamepad1.dpad_up) {
                        TleftDrive.setPower(0);
                        BleftDrive.setPower(0);
                        TrightDrive.setPower(0);
                        BrightDrive.setPower(0);
                        power = 0.5;
                        break;
                    }
                }


                while (gamepad1.dpad_down) {
                    TleftDrive.setPower(power);
                    BleftDrive.setPower(power);
                    TrightDrive.setPower(power);
                    BrightDrive.setPower(power);
                    if (gamepad1.left_trigger != 0)
                        power = 0.5;
                    if (gamepad1.left_trigger == 0)
                        power = 0.3;
                    if (!gamepad1.dpad_down) {
                        TleftDrive.setPower(0);
                        BleftDrive.setPower(0);
                        TrightDrive.setPower(0);
                        BrightDrive.setPower(0);
                        power = 0.5;
                        break;
                    }
                }

                while (gamepad1.dpad_left) {
                    TleftDrive.setPower(power);
                    BleftDrive.setPower(-power);
                    TrightDrive.setPower(-power);
                    BrightDrive.setPower(power);
                    if (gamepad1.left_trigger != 0)
                        power = 0.9;
                    if (gamepad1.left_trigger == 0)
                        power = 0.5;
                    if (!gamepad1.dpad_left) {
                        TleftDrive.setPower(0);
                        BleftDrive.setPower(0);
                        TrightDrive.setPower(0);
                        BrightDrive.setPower(0);
                        power = 0.9;
                        break;
                    }
                }

                while (gamepad1.dpad_right) {
                    TleftDrive.setPower(-power);
                    BleftDrive.setPower(power);
                    TrightDrive.setPower(power);
                    BrightDrive.setPower(-power);
                    if (gamepad1.left_trigger != 0)
                        power = 0.9;
                    if (gamepad1.left_trigger == 0)
                        power = 0.5;
                    if (!gamepad1.dpad_right) {
                        TleftDrive.setPower(0);
                        BleftDrive.setPower(0);
                        TrightDrive.setPower(0);
                        BrightDrive.setPower(0);
                        power = 0.9;
                        break;
                    }
                }

                while (gamepad1.y) {
                    TleftDrive.setPower(0.7);
                    BrightDrive.setPower(0.7);
                    if (!(gamepad1.y)) {
                        TleftDrive.setPower(0);
                        BrightDrive.setPower(0);
                        break;
                    }
                }
                while (gamepad1.a) {
                    BleftDrive.setPower(0.7);
                    TrightDrive.setPower(0.7);
                    if (!(gamepad1.a)) {
                        BleftDrive.setPower(0);
                        TrightDrive.setPower(0);
                        break;
                    }
                }
                while (gamepad1.x) {
                    TleftDrive.setPower(-0.7);
                    BrightDrive.setPower(-0.7);
                    if (!(gamepad1.x)) {
                        TleftDrive.setPower(0);
                        BrightDrive.setPower(0);
                        break;
                    }
                }
                while (gamepad1.b) {
                    BleftDrive.setPower(-0.7);
                    TrightDrive.setPower(-0.7);
                    if (!(gamepad1.b)) {
                        BleftDrive.setPower(0);
                        TrightDrive.setPower(0);
                        break;
                    }
                }
                while (gamepad1.left_bumper) {
                    TleftDrive.setPower(0.35);
                    BleftDrive.setPower(0.35);
                    TrightDrive.setPower(-0.35);
                    BrightDrive.setPower(-0.35);
                    if (!gamepad1.right_bumper) {
                        TleftDrive.setPower(0);
                        BleftDrive.setPower(0);
                        TrightDrive.setPower(0);
                        BrightDrive.setPower(0);
                        break;
                    }
                }
                while (gamepad1.right_bumper) {
                    TleftDrive.setPower(-0.35);
                    BleftDrive.setPower(-0.35);
                    TrightDrive.setPower(0.35);
                    BrightDrive.setPower(0.35);
                    if (!gamepad1.left_bumper) {
                        TleftDrive.setPower(0);
                        BleftDrive.setPower(0);
                        TrightDrive.setPower(0);
                        BrightDrive.setPower(0);
                        break;
                    }
                }
            }

            telemetry.addData("pozitie", ServoRight.getPosition());
            telemetry.addData("pozitie", ServoLeft.getPosition());
            telemetry.update();
        }

    }

}
