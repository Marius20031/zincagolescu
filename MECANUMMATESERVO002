package org.firstinspires.ftc.teamcode;

import com.qualcomm.robotcore.eventloop.opmode.LinearOpMode;
import com.qualcomm.robotcore.eventloop.opmode.TeleOp;
import com.qualcomm.robotcore.hardware.DcMotor;
import com.qualcomm.robotcore.hardware.DcMotorSimple;
import com.qualcomm.robotcore.hardware.Servo;
import com.qualcomm.robotcore.hardware.CRServo;
import com.qualcomm.robotcore.util.ElapsedTime;


/**
 * This file contains an minimal example of a Linear "OpMode". An OpMode is a 'program' that runs in either
 * the autonomous or the teleop period of an FTC match. The names of OpModes appear on the menu
 * of the FTC Driver Station. When an selection is made from the menu, the corresponding OpMode
 * class is instantiated on the Robot Controller and executed.
 *
 * This particular OpMode just executes a basic Tank Drive Teleop for a two wheeled robot
 * It includes all the skeletal structure that all linear OpModes contain.
 *
 * Use Android Studios to Copy this Class, and Paste it into your team's code folder with a new name.
 * Remove or comment out the @Disabled line to add this opmode to the Driver Station OpMode list
 */

@TeleOp(name="MECANUMMATESERVO", group="Linear Opmode")

public class MECANUMMATESERVO extends LinearOpMode {

    CRServo Paralelogram;
    CRServo Basculare;
    // Declare OpMode members.
    private ElapsedTime runtime = new ElapsedTime();
    private DcMotor TleftDrive = null;
    private DcMotor BleftDrive = null;
    private DcMotor TrightDrive = null;
    private DcMotor BrightDrive = null;
    private DcMotor BratHeavy1 = null;
    private DcMotor BratHeavy2 = null;
    private Servo ServoRight = null;
    private Servo Grip=null;
    private Servo Servomotor2 = null;
    private  Servo ServoLeft = null;
    private Servo Servo_spateV = null;
    private Servo Servo_spateE = null;

    double  power   = 0.5;
    float ok1=1;
    float ok2=1;
    double p=0.1;

    @Override
    public void runOpMode() {
        telemetry.addData("Status", "Initialized");
        telemetry.update();

        // Initialize the hardware variables. Note that the strings used here as parameters
        // to 'get' must correspond to the names assigned during the robot configuration
        // step (using the FTC Robot Controller app on the phone).
        TleftDrive = hardwareMap.get(DcMotor.class, "Tleft_drive");
        TrightDrive = hardwareMap.get(DcMotor.class, "Tright_drive");
        BleftDrive = hardwareMap.get(DcMotor.class, "Bleft_drive");
        BrightDrive = hardwareMap.get(DcMotor.class, "Bright_drive");
        BratHeavy1 = hardwareMap.get(DcMotor.class, "BratHeavy1");
        BratHeavy2 = hardwareMap.get(DcMotor.class, "BratHeavy2");
        Grip = hardwareMap.get(Servo.class, "Grip");
        Paralelogram = hardwareMap.get(CRServo.class, "Paralelogram");
        Basculare = hardwareMap.get(CRServo.class, "Basculare");
        ServoLeft = hardwareMap.get(Servo.class, "Servo1");
        ServoRight = hardwareMap.get(Servo.class, "Servo2");
        Servo_spateE = hardwareMap.get(Servo.class, "Servo_spateE");
        Servo_spateV = hardwareMap.get(Servo.class, "Servo_spateV");


        TleftDrive.setZeroPowerBehavior(DcMotor.ZeroPowerBehavior.BRAKE);
        TrightDrive.setZeroPowerBehavior(DcMotor.ZeroPowerBehavior.BRAKE);
        BrightDrive.setZeroPowerBehavior(DcMotor.ZeroPowerBehavior.BRAKE);
        BleftDrive.setZeroPowerBehavior(DcMotor.ZeroPowerBehavior.BRAKE);
        BratHeavy1.setZeroPowerBehavior(DcMotor.ZeroPowerBehavior.BRAKE);
        BratHeavy2.setZeroPowerBehavior(DcMotor.ZeroPowerBehavior.BRAKE);

        TleftDrive.setDirection(DcMotorSimple.Direction.REVERSE);
        BleftDrive.setDirection(DcMotorSimple.Direction.REVERSE);


        // Wait for the game to start (driver presses PLAY)
        waitForStart();
        runtime.reset();

        // run until the end of the match (driver presses STOP)
        while (opModeIsActive()) {

            if (gamepad1.start){

                ServoRight.setPosition(1);
            }

            while (gamepad1.dpad_up) {
                TleftDrive.setPower(-power);
                BleftDrive.setPower(-power);
                TrightDrive.setPower(-power);
                BrightDrive.setPower(-power);
                if (gamepad1.left_trigger != 0)
                    power = 0.5;
                if (gamepad1.left_trigger == 0)
                    power = 0.3;
                if (!gamepad1.dpad_up) {
                    TleftDrive.setPower(0);
                    BleftDrive.setPower(0);
                    TrightDrive.setPower(0);
                    BrightDrive.setPower(0);
                    power = 0.5;
                    break;
                }
                while (gamepad2.left_bumper) {
                    BratHeavy1.setPower(0.5);
                    if (!gamepad2.left_bumper) {
                        BratHeavy1.setPower(0);
                        break;
                    }

                }

                while (gamepad2.right_bumper) {
                    BratHeavy2.setPower(0.5);
                    if (!gamepad2.right_bumper) {
                        BratHeavy2.setPower(0);
                        break;
                    }

                }

                while (gamepad2.start) {
                    BratHeavy2.setPower(-0.5);
                    if (!gamepad2.start) {
                        BratHeavy2.setPower(0);
                        break;
                    }
                }
                while (gamepad2.back) {
                    BratHeavy1.setPower(-0.5);
                    if (!gamepad2.back) {
                        BratHeavy1.setPower(0);
                        break;
                    }

                }
                if (gamepad2.dpad_right) {
                    Grip.setPosition(0.4);
                }
                if (gamepad2.x) {
                    Servo_spateE.setPosition(0.5);
                    Servo_spateV.setPosition(0.5);
                }
                if (gamepad2.b) {
                    Servo_spateE.setPosition(0);
                    Servo_spateV.setPosition(0);
                }
                if (gamepad2.dpad_left) {
                    Grip.setPosition(-0.4);
                }
                while (gamepad2.dpad_down) {
                    Basculare.setPower(1);
                    sleep(100);
                    if(!(gamepad2.dpad_down)) {
                        Basculare.setPower(0);}

                }
                while (gamepad2.dpad_up) {
                    Basculare.setPower(-1);
                    sleep(100);
                    if (!(gamepad2.dpad_up)) {
                        Basculare.setPower(0);
                    }
                }
                while (gamepad2.a) {
                    Paralelogram.setPower(1);
                    sleep(100);
                    if(!(gamepad2.a)) {
                        Paralelogram.setPower(0);}

                }
                while (gamepad2.y) {
                    Paralelogram.setPower(-1);
                    sleep(100);
                    if(!(gamepad2.y)) {
                        Paralelogram.setPower(0);}
                }
            }


            while (gamepad1.dpad_down) {
                TleftDrive.setPower(power);
                BleftDrive.setPower(power);
                TrightDrive.setPower(power);
                BrightDrive.setPower(power);
                if (gamepad1.left_trigger != 0)
                    power = 0.5;
                if (gamepad1.left_trigger == 0)
                    power = 0.3;
                if (!gamepad1.dpad_down) {
                    TleftDrive.setPower(0);
                    BleftDrive.setPower(0);
                    TrightDrive.setPower(0);
                    BrightDrive.setPower(0);
                    power = 0.5;
                    break;
                }
                while (gamepad2.left_bumper) {
                    BratHeavy1.setPower(0.5);
                    if (!gamepad2.left_bumper) {
                        BratHeavy1.setPower(0);
                        break;
                    }

                }

                while (gamepad2.right_bumper) {
                    BratHeavy2.setPower(0.5);
                    if (!gamepad2.right_bumper) {
                        BratHeavy2.setPower(0);
                        break;
                    }

                }

                while (gamepad2.start) {
                    BratHeavy2.setPower(-0.5);
                    if (!gamepad2.start) {
                        BratHeavy2.setPower(0);
                        break;
                    }
                }
                while (gamepad2.back) {
                    BratHeavy1.setPower(-0.5);
                    if (!gamepad2.back) {
                        BratHeavy1.setPower(0);
                        break;
                    }

                }
                if (gamepad2.dpad_right) {
                    Grip.setPosition(0.4);
                }
                if (gamepad2.x) {
                    Servo_spateE.setPosition(0.5);
                    Servo_spateV.setPosition(0.5);
                }
                if (gamepad2.b) {
                    Servo_spateE.setPosition(0);
                    Servo_spateV.setPosition(0);
                }
                if (gamepad2.dpad_left) {
                    Grip.setPosition(-0.4);
                }
                while (gamepad2.dpad_down) {
                    Basculare.setPower(1);
                    sleep(100);
                    if(!(gamepad2.dpad_down)) {
                        Basculare.setPower(0);}

                }
                while (gamepad2.dpad_up) {
                    Basculare.setPower(-1);
                    sleep(100);
                    if (!(gamepad2.dpad_up)) {
                        Basculare.setPower(0);
                    }
                }
                while (gamepad2.a) {
                    Paralelogram.setPower(1);
                    sleep(100);
                    if(!(gamepad2.a)) {
                        Paralelogram.setPower(0);}

                }
                while (gamepad2.y) {
                    Paralelogram.setPower(-1);
                    sleep(100);
                    if(!(gamepad2.y)) {
                        Paralelogram.setPower(0);}
                }
            }

            while (gamepad1.dpad_left) {
                TleftDrive.setPower(power);
                BleftDrive.setPower(-power);
                TrightDrive.setPower(-power);
                BrightDrive.setPower(power);
                if (gamepad1.left_trigger != 0)
                    power = 0.9;
                if (gamepad1.left_trigger == 0)
                    power = 0.5;
                if (!gamepad1.dpad_left) {
                    TleftDrive.setPower(0);
                    BleftDrive.setPower(0);
                    TrightDrive.setPower(0);
                    BrightDrive.setPower(0);
                    power = 0.9;
                    break;
                }
                while (gamepad2.left_bumper) {
                    BratHeavy1.setPower(0.5);
                    if (!gamepad2.left_bumper) {
                        BratHeavy1.setPower(0);
                        break;
                    }

                }

                while (gamepad2.right_bumper) {
                    BratHeavy2.setPower(0.5);
                    if (!gamepad2.right_bumper) {
                        BratHeavy2.setPower(0);
                        break;
                    }

                }

                while (gamepad2.start) {
                    BratHeavy2.setPower(-0.5);
                    if (!gamepad2.start) {
                        BratHeavy2.setPower(0);
                        break;
                    }
                }
                while (gamepad2.back) {
                    BratHeavy1.setPower(-0.5);
                    if (!gamepad2.back) {
                        BratHeavy1.setPower(0);
                        break;
                    }

                }
                if (gamepad2.dpad_right) {
                    Grip.setPosition(0.4);
                }
                if (gamepad2.x) {
                    Servo_spateE.setPosition(0.5);
                    Servo_spateV.setPosition(0.5);
                }
                if (gamepad2.b) {
                    Servo_spateE.setPosition(0);
                    Servo_spateV.setPosition(0);
                }
                if (gamepad2.dpad_left) {
                    Grip.setPosition(-0.4);
                }
                while (gamepad2.dpad_down) {
                    Basculare.setPower(1);
                    sleep(100);
                    if(!(gamepad2.dpad_down)) {
                        Basculare.setPower(0);}

                }
                while (gamepad2.dpad_up) {
                    Basculare.setPower(-1);
                    sleep(100);
                    if (!(gamepad2.dpad_up)) {
                        Basculare.setPower(0);
                    }
                }
                while (gamepad2.a) {
                    Paralelogram.setPower(1);
                    sleep(100);
                    if(!(gamepad2.a)) {
                        Paralelogram.setPower(0);}

                }
                while (gamepad2.y) {
                    Paralelogram.setPower(-1);
                    sleep(100);
                    if(!(gamepad2.y)) {
                        Paralelogram.setPower(0);}
                }
            }

            while (gamepad1.dpad_right) {
                TleftDrive.setPower(-power);
                BleftDrive.setPower(power);
                TrightDrive.setPower(power);
                BrightDrive.setPower(-power);
                if (gamepad1.left_trigger != 0)
                    power = 0.9;
                if (gamepad1.left_trigger == 0)
                    power = 0.5;
                if (!gamepad1.dpad_right) {
                    TleftDrive.setPower(0);
                    BleftDrive.setPower(0);
                    TrightDrive.setPower(0);
                    BrightDrive.setPower(0);
                    power = 0.9;
                    break;
                }
                while (gamepad2.left_bumper) {
                    BratHeavy1.setPower(0.5);
                    if (!gamepad2.left_bumper) {
                        BratHeavy1.setPower(0);
                        break;
                    }

                }

                while (gamepad2.right_bumper) {
                    BratHeavy2.setPower(0.5);
                    if (!gamepad2.right_bumper) {
                        BratHeavy2.setPower(0);
                        break;
                    }

                }

                while (gamepad2.start) {
                    BratHeavy2.setPower(-0.5);
                    if (!gamepad2.start) {
                        BratHeavy2.setPower(0);
                        break;
                    }
                }
                while (gamepad2.back) {
                    BratHeavy1.setPower(-0.5);
                    if (!gamepad2.back) {
                        BratHeavy1.setPower(0);
                        break;
                    }

                }
                if (gamepad2.dpad_right) {
                    Grip.setPosition(0.4);
                }
                if (gamepad2.x) {
                    Servo_spateE.setPosition(0.5);
                    Servo_spateV.setPosition(0.5);
                }
                if (gamepad2.b) {
                    Servo_spateE.setPosition(0);
                    Servo_spateV.setPosition(0);
                }
                if (gamepad2.dpad_left) {
                    Grip.setPosition(-0.4);
                }
                while (gamepad2.dpad_down) {
                    Basculare.setPower(1);
                    sleep(100);
                    if(!(gamepad2.dpad_down)) {
                        Basculare.setPower(0);}

                }
                while (gamepad2.dpad_up) {
                    Basculare.setPower(-1);
                    sleep(100);
                    if (!(gamepad2.dpad_up)) {
                        Basculare.setPower(0);
                    }
                }
                while (gamepad2.a) {
                    Paralelogram.setPower(1);
                    sleep(100);
                    if(!(gamepad2.a)) {
                        Paralelogram.setPower(0);}

                }
                while (gamepad2.y) {
                    Paralelogram.setPower(-1);
                    sleep(100);
                    if(!(gamepad2.y)) {
                        Paralelogram.setPower(0);}
                }
            }

            while (gamepad1.y) {
                TleftDrive.setPower(0.7);
                BrightDrive.setPower(0.7);
                if (!(gamepad1.y)) {
                    TleftDrive.setPower(0);
                    BrightDrive.setPower(0);
                    break;
                }
                while (gamepad2.left_bumper) {
                    BratHeavy1.setPower(0.5);
                    if (!gamepad2.left_bumper) {
                        BratHeavy1.setPower(0);
                        break;
                    }

                }

                while (gamepad2.right_bumper) {
                    BratHeavy2.setPower(0.5);
                    if (!gamepad2.right_bumper) {
                        BratHeavy2.setPower(0);
                        break;
                    }

                }

                while (gamepad2.start) {
                    BratHeavy2.setPower(-0.5);
                    if (!gamepad2.start) {
                        BratHeavy2.setPower(0);
                        break;
                    }
                }
                while (gamepad2.back) {
                    BratHeavy1.setPower(-0.5);
                    if (!gamepad2.back) {
                        BratHeavy1.setPower(0);
                        break;
                    }

                }
                if (gamepad2.dpad_right) {
                    Grip.setPosition(0.4);
                }
                if (gamepad2.x) {
                    Servo_spateE.setPosition(0.5);
                    Servo_spateV.setPosition(0.5);
                }
                if (gamepad2.b) {
                    Servo_spateE.setPosition(0);
                    Servo_spateV.setPosition(0);
                }
                if (gamepad2.dpad_left) {
                    Grip.setPosition(-0.4);
                }
                while (gamepad2.dpad_down) {
                    Basculare.setPower(1);
                    sleep(100);
                    if(!(gamepad2.dpad_down)) {
                        Basculare.setPower(0);}

                }
                while (gamepad2.dpad_up) {
                    Basculare.setPower(-1);
                    sleep(100);
                    if (!(gamepad2.dpad_up)) {
                        Basculare.setPower(0);
                    }
                }
                while (gamepad2.a) {
                    Paralelogram.setPower(1);
                    sleep(100);
                    if(!(gamepad2.a)) {
                        Paralelogram.setPower(0);}

                }
                while (gamepad2.y) {
                    Paralelogram.setPower(-1);
                    sleep(100);
                    if(!(gamepad2.y)) {
                        Paralelogram.setPower(0);}
                }
            }
            while (gamepad1.a) {
                BleftDrive.setPower(0.7);
                TrightDrive.setPower(0.7);
                if (!(gamepad1.a)) {
                    BleftDrive.setPower(0);
                    TrightDrive.setPower(0);
                    break;
                }
                while (gamepad2.left_bumper) {
                    BratHeavy1.setPower(0.5);
                    if (!gamepad2.left_bumper) {
                        BratHeavy1.setPower(0);
                        break;
                    }

                }

                while (gamepad2.right_bumper) {
                    BratHeavy2.setPower(0.5);
                    if (!gamepad2.right_bumper) {
                        BratHeavy2.setPower(0);
                        break;
                    }

                }

                while (gamepad2.start) {
                    BratHeavy2.setPower(-0.5);
                    if (!gamepad2.start) {
                        BratHeavy2.setPower(0);
                        break;
                    }
                }
                while (gamepad2.back) {
                    BratHeavy1.setPower(-0.5);
                    if (!gamepad2.back) {
                        BratHeavy1.setPower(0);
                        break;
                    }

                }
                if (gamepad2.dpad_right) {
                    Grip.setPosition(0.4);
                }
                if (gamepad2.x) {
                    Servo_spateE.setPosition(0.5);
                    Servo_spateV.setPosition(0.5);
                }
                if (gamepad2.b) {
                    Servo_spateE.setPosition(0);
                    Servo_spateV.setPosition(0);
                }
                if (gamepad2.dpad_left) {
                    Grip.setPosition(-0.4);
                }
                while (gamepad2.dpad_down) {
                    Basculare.setPower(1);
                    sleep(100);
                    if(!(gamepad2.dpad_down)) {
                        Basculare.setPower(0);}

                }
                while (gamepad2.dpad_up) {
                    Basculare.setPower(-1);
                    sleep(100);
                    if (!(gamepad2.dpad_up)) {
                        Basculare.setPower(0);
                    }
                }
                while (gamepad2.a) {
                    Paralelogram.setPower(1);
                    sleep(100);
                    if(!(gamepad2.a)) {
                        Paralelogram.setPower(0);}

                }
                while (gamepad2.y) {
                    Paralelogram.setPower(-1);
                    sleep(100);
                    if(!(gamepad2.y)) {
                        Paralelogram.setPower(0);}
                }
            }
            while (gamepad1.x) {
                TleftDrive.setPower(-0.7);
                BrightDrive.setPower(-0.7);
                if (!(gamepad1.x)) {
                    TleftDrive.setPower(0);
                    BrightDrive.setPower(0);
                    break;
                }
                while (gamepad2.left_bumper) {
                    BratHeavy1.setPower(0.5);
                    if (!gamepad2.left_bumper) {
                        BratHeavy1.setPower(0);
                        break;
                    }

                }

                while (gamepad2.right_bumper) {
                    BratHeavy2.setPower(0.5);
                    if (!gamepad2.right_bumper) {
                        BratHeavy2.setPower(0);
                        break;
                    }

                }

                while (gamepad2.start) {
                    BratHeavy2.setPower(-0.5);
                    if (!gamepad2.start) {
                        BratHeavy2.setPower(0);
                        break;
                    }
                }
                while (gamepad2.back) {
                    BratHeavy1.setPower(-0.5);
                    if (!gamepad2.back) {
                        BratHeavy1.setPower(0);
                        break;
                    }

                }
                if (gamepad2.dpad_right) {
                    Grip.setPosition(0.4);
                }
                if (gamepad2.x) {
                    Servo_spateE.setPosition(0.5);
                    Servo_spateV.setPosition(0.5);
                }
                if (gamepad2.b) {
                    Servo_spateE.setPosition(0);
                    Servo_spateV.setPosition(0);
                }
                if (gamepad2.dpad_left) {
                    Grip.setPosition(-0.4);
                }
                while (gamepad2.dpad_down) {
                    Basculare.setPower(1);
                    sleep(100);
                    if(!(gamepad2.dpad_down)) {
                        Basculare.setPower(0);}

                }
                while (gamepad2.dpad_up) {
                    Basculare.setPower(-1);
                    sleep(100);
                    if (!(gamepad2.dpad_up)) {
                        Basculare.setPower(0);
                    }
                }
                while (gamepad2.a) {
                    Paralelogram.setPower(1);
                    sleep(100);
                    if(!(gamepad2.a)) {
                        Paralelogram.setPower(0);}

                }
                while (gamepad2.y) {
                    Paralelogram.setPower(-1);
                    sleep(100);
                    if(!(gamepad2.y)) {
                        Paralelogram.setPower(0);}
                }
            }
            while (gamepad1.b) {
                BleftDrive.setPower(-0.7);
                TrightDrive.setPower(-0.7);
                if (!(gamepad1.b)) {
                    BleftDrive.setPower(0);
                    TrightDrive.setPower(0);
                    break;
                }
                while (gamepad2.left_bumper) {
                    BratHeavy1.setPower(0.5);
                    if (!gamepad2.left_bumper) {
                        BratHeavy1.setPower(0);
                        break;
                    }

                }

                while (gamepad2.right_bumper) {
                    BratHeavy2.setPower(0.5);
                    if (!gamepad2.right_bumper) {
                        BratHeavy2.setPower(0);
                        break;
                    }

                }

                while (gamepad2.start) {
                    BratHeavy2.setPower(-0.5);
                    if (!gamepad2.start) {
                        BratHeavy2.setPower(0);
                        break;
                    }
                }
                while (gamepad2.back) {
                    BratHeavy1.setPower(-0.5);
                    if (!gamepad2.back) {
                        BratHeavy1.setPower(0);
                        break;
                    }

                }
                if (gamepad2.dpad_right) {
                    Grip.setPosition(0.4);
                }
                if (gamepad2.x) {
                    Servo_spateE.setPosition(0.5);
                    Servo_spateV.setPosition(0.5);
                }
                if (gamepad2.b) {
                    Servo_spateE.setPosition(0);
                    Servo_spateV.setPosition(0);
                }
                if (gamepad2.dpad_left) {
                    Grip.setPosition(-0.4);
                }
                while (gamepad2.dpad_down) {
                    Basculare.setPower(1);
                    sleep(100);
                    if(!(gamepad2.dpad_down)) {
                        Basculare.setPower(0);}

                }
                while (gamepad2.dpad_up) {
                    Basculare.setPower(-1);
                    sleep(100);
                    if (!(gamepad2.dpad_up)) {
                        Basculare.setPower(0);
                    }
                }
                while (gamepad2.a) {
                    Paralelogram.setPower(1);
                    sleep(100);
                    if(!(gamepad2.a)) {
                        Paralelogram.setPower(0);}

                }
                while (gamepad2.y) {
                    Paralelogram.setPower(-1);
                    sleep(100);
                    if(!(gamepad2.y)) {
                        Paralelogram.setPower(0);}
                }
            }
            while (gamepad1.left_bumper) {
                TleftDrive.setPower(0.35);
                BleftDrive.setPower(0.35);
                TrightDrive.setPower(-0.35);
                BrightDrive.setPower(-0.35);
                if (!gamepad1.right_bumper) {
                    TleftDrive.setPower(0);
                    BleftDrive.setPower(0);
                    TrightDrive.setPower(0);
                    BrightDrive.setPower(0);
                    break;
                }
                while (gamepad2.left_bumper) {
                    BratHeavy1.setPower(0.5);
                    if (!gamepad2.left_bumper) {
                        BratHeavy1.setPower(0);
                        break;
                    }

                }

                while (gamepad2.right_bumper) {
                    BratHeavy2.setPower(0.5);
                    if (!gamepad2.right_bumper) {
                        BratHeavy2.setPower(0);
                        break;
                    }

                }

                while (gamepad2.start) {
                    BratHeavy2.setPower(-0.5);
                    if (!gamepad2.start) {
                        BratHeavy2.setPower(0);
                        break;
                    }
                }
                while (gamepad2.back) {
                    BratHeavy1.setPower(-0.5);
                    if (!gamepad2.back) {
                        BratHeavy1.setPower(0);
                        break;
                    }

                }
                if (gamepad2.dpad_right) {
                    Grip.setPosition(0.4);
                }
                if (gamepad2.x) {
                    Servo_spateE.setPosition(0.5);
                    Servo_spateV.setPosition(0.5);
                }
                if (gamepad2.b) {
                    Servo_spateE.setPosition(0);
                    Servo_spateV.setPosition(0);
                }
                if (gamepad2.dpad_left) {
                    Grip.setPosition(-0.4);
                }
                while (gamepad2.dpad_down) {
                    Basculare.setPower(1);
                    sleep(100);
                    if(!(gamepad2.dpad_down)) {
                        Basculare.setPower(0);}

                }
                while (gamepad2.dpad_up) {
                    Basculare.setPower(-1);
                    sleep(100);
                    if (!(gamepad2.dpad_up)) {
                        Basculare.setPower(0);
                    }
                }
                while (gamepad2.a) {
                    Paralelogram.setPower(1);
                    sleep(100);
                    if(!(gamepad2.a)) {
                        Paralelogram.setPower(0);}

                }
                while (gamepad2.y) {
                    Paralelogram.setPower(-1);
                    sleep(100);
                    if(!(gamepad2.y)) {
                        Paralelogram.setPower(0);}
                }
            }
            while (gamepad1.right_bumper) {
                TleftDrive.setPower(-0.35);
                BleftDrive.setPower(-0.35);
                TrightDrive.setPower(0.35);
                BrightDrive.setPower(0.35);
                if (!gamepad1.left_bumper) {
                    TleftDrive.setPower(0);
                    BleftDrive.setPower(0);
                    TrightDrive.setPower(0);
                    BrightDrive.setPower(0);
                    break;
                }
